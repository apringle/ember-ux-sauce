{{yield (hash
  subheading=(component "uxs-subheading" base="uxs-menu__subheading")
  item=(
    component
    "uxs-menu/item"
    hasRightIcon=hasRightIcon
    hasLeftIcon=hasLeftIcon
    hasSecondaryText=hasSecondaryText
    style=style
    horizontal=horizontal
  )
  link=(
    component
    "uxs-menu/link"
    hasRightIcon=hasRightIcon
    hasLeftIcon=hasLeftIcon
    hasSecondaryText=hasSecondaryText
    style=style
    horizontal=horizontal
  )
  divider=(component "uxs-div" class=(bem b "divider" m) style=style horizontal=horizontal)
)}}
